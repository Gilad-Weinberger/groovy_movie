{
  "builds": [
    {
      "src": "groovy_movie/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "staticfiles/*",
      "use": "@vercel/staticfiles"
    }
  ],
  "routes": [
    {
      "src": "/staticfiles/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "groovy_movie/wsgi.py"
    }
  ]
}
